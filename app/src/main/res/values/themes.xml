<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AsetDSI" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/ijau</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/ijau</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="DatePicker" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/ijau</item>
        <item name="colorOnPrimary">@color/ijau</item>
    </style>

    <style name="TimePicker" parent="Theme.MaterialComponents.Light.Dialog">
        <item name="colorAccent">@color/ijau</item>
        <item name="colorOnPrimary">@color/ijau</item>
    </style>

        <style name="TextAppearance.AppCompat.Widget.ActionBar.Title" parent="@android:style/TextAppearance">
            <item name="android:textSize">16sp</item>
            <item name="maxLines">3</item>
            <item name="android:lines">2</item>

        </style>

